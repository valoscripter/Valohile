local Players=game:GetService("Players")local LocalPlayer=Players.LocalPlayer local TweenService=game:GetService("TweenService")local RunService=game:GetService("RunService")local ChatGui=Instance.new("ScreenGui")ChatGui.Name="PremiumChatLogger"ChatGui.Parent=LocalPlayer.PlayerGui ChatGui.ResetOnSpawn=false local MainWindow=Instance.new("Frame")MainWindow.Name="MainWindow"MainWindow.Parent=ChatGui MainWindow.BackgroundColor3=Color3.fromRGB(15,15,20)MainWindow.BorderSizePixel=0 MainWindow.Position=UDim2.new(0.1,0,0.5,-150)MainWindow.Size=UDim2.new(0,350,0,40)MainWindow.Active=true MainWindow.Draggable=true local UICorner=Instance.new("UICorner")UICorner.CornerRadius=UDim.new(0,12)UICorner.Parent=MainWindow local TitleBar=Instance.new("Frame")TitleBar.Name="TitleBar"TitleBar.Parent=MainWindow TitleBar.BackgroundColor3=Color3.fromRGB(25,25,30)TitleBar.BorderSizePixel=0 TitleBar.Size=UDim2.new(1,0,0,30)local TitleUICorner=Instance.new("UICorner")TitleUICorner.CornerRadius=UDim.new(0,8)TitleUICorner.Parent=TitleBar local TitleLabel=Instance.new("TextLabel")TitleLabel.Name="Title"TitleLabel.Parent=TitleBar TitleLabel.BackgroundTransparency=1 TitleLabel.Position=UDim2.new(0,15,0,0)TitleLabel.Size=UDim2.new(0,200,1,0)TitleLabel.Font=Enum.Font.GothamSemibold TitleLabel.Text="CHAT LOGGER"TitleLabel.TextColor3=Color3.fromRGB(220,220,220)TitleLabel.TextSize=14 TitleLabel.TextXAlignment=Enum.TextXAlignment.Left local TrackToggle=Instance.new("TextButton")TrackToggle.Name="TrackToggle"TrackToggle.Parent=TitleBar TrackToggle.BackgroundColor3=Color3.fromRGB(35,35,40)TrackToggle.BorderSizePixel=0 TrackToggle.Position=UDim2.new(0.5,-50,0,5)TrackToggle.Size=UDim2.new(0,100,0,20)TrackToggle.Font=Enum.Font.GothamMedium TrackToggle.Text="TRACK:ON"TrackToggle.TextColor3=Color3.fromRGB(150,255,150)TrackToggle.TextSize=12 local TrackUICorner=Instance.new("UICorner")TrackUICorner.CornerRadius=UDim.new(0,6)TrackUICorner.Parent=TrackToggle local CloseButton=Instance.new("TextButton")CloseButton.Name="Close"CloseButton.Parent=TitleBar CloseButton.BackgroundColor3=Color3.fromRGB(200,50,50)CloseButton.BorderSizePixel=0 CloseButton.Position=UDim2.new(1,-30,0,5)CloseButton.Size=UDim2.new(0,20,0,20)CloseButton.Font=Enum.Font.GothamMedium CloseButton.Text="×"CloseButton.TextColor3=Color3.fromRGB(255,255,255)CloseButton.TextSize=16 local CloseUICorner=Instance.new("UICorner")CloseUICorner.CornerRadius=UDim.new(0,6)CloseUICorner.Parent=CloseButton local MinimizeButton=Instance.new("TextButton")MinimizeButton.Name="Minimize"MinimizeButton.Parent=TitleBar MinimizeButton.BackgroundColor3=Color3.fromRGB(50,50,55)MinimizeButton.BorderSizePixel=0 MinimizeButton.Position=UDim2.new(1,-60,0,5)MinimizeButton.Size=UDim2.new(0,20,0,20)MinimizeButton.Font=Enum.Font.GothamMedium MinimizeButton.Text="-"MinimizeButton.TextColor3=Color3.fromRGB(255,255,255)MinimizeButton.TextSize=16 local MinimizeUICorner=Instance.new("UICorner")MinimizeUICorner.CornerRadius=UDim.new(0,6)MinimizeUICorner.Parent=MinimizeButton local ContentFrame=Instance.new("Frame")ContentFrame.Name="Content"ContentFrame.Parent=MainWindow ContentFrame.BackgroundColor3=Color3.fromRGB(20,20,25)ContentFrame.BorderSizePixel=0 ContentFrame.Position=UDim2.new(0,0,0,30)ContentFrame.Size=UDim2.new(1,0,1,-30)ContentFrame.Visible=false local ContentUICorner=Instance.new("UICorner")ContentUICorner.CornerRadius=UDim.new(0,8)ContentUICorner.Parent=ContentFrame local TabButtons=Instance.new("Frame")TabButtons.Name="TabButtons"TabButtons.Parent=ContentFrame TabButtons.BackgroundColor3=Color3.fromRGB(30,30,35)TabButtons.BorderSizePixel=0 TabButtons.Size=UDim2.new(1,0,0,35)local TabUICorner=Instance.new("UICorner")TabUICorner.CornerRadius=UDim.new(0,6)TabUICorner.Parent=TabButtons local ChatTab=Instance.new("TextButton")ChatTab.Name="ChatTab"ChatTab.Parent=TabButtons ChatTab.BackgroundColor3=Color3.fromRGB(50,50,60)ChatTab.BorderSizePixel=0 ChatTab.Position=UDim2.new(0.02,0,0.15,0)ChatTab.Size=UDim2.new(0.48,0,0.7,0)ChatTab.Font=Enum.Font.GothamMedium ChatTab.Text="CHAT"ChatTab.TextColor3=Color3.fromRGB(255,255,255)ChatTab.TextSize=12 local ChatTabUICorner=Instance.new("UICorner")ChatTabUICorner.CornerRadius=UDim.new(0,8)ChatTabUICorner.Parent=ChatTab local CommandsTab=Instance.new("TextButton")CommandsTab.Name="CommandsTab"CommandsTab.Parent=TabButtons CommandsTab.BackgroundColor3=Color3.fromRGB(35,35,40)CommandsTab.BorderSizePixel=0 CommandsTab.Position=UDim2.new(0.51,0,0.15,0)CommandsTab.Size=UDim2.new(0.48,0,0.7,0)CommandsTab.Font=Enum.Font.GothamMedium CommandsTab.Text="COMMANDS"CommandsTab.TextColor3=Color3.fromRGB(200,200,200)CommandsTab.TextSize=12 local CommandsTabUICorner=Instance.new("UICorner")CommandsTabUICorner.CornerRadius=UDim.new(0,8)CommandsTabUICorner.Parent=CommandsTab local LogContainer=Instance.new("Frame")LogContainer.Name="LogContainer"LogContainer.Parent=ContentFrame LogContainer.BackgroundColor3=Color3.fromRGB(15,15,20)LogContainer.BorderSizePixel=0 LogContainer.Position=UDim2.new(0,0,0,35)LogContainer.Size=UDim2.new(1,0,1,-35)local LogUICorner=Instance.new("UICorner")LogUICorner.CornerRadius=UDim.new(0,8)LogUICorner.Parent=LogContainer local ChatLog=Instance.new("ScrollingFrame")ChatLog.Name="ChatLog"ChatLog.Parent=LogContainer ChatLog.BackgroundTransparency=1 ChatLog.BorderSizePixel=0 ChatLog.Size=UDim2.new(1,0,1,0)ChatLog.ScrollBarThickness=4 ChatLog.CanvasSize=UDim2.new(0,0,0,0)ChatLog.AutomaticCanvasSize=Enum.AutomaticSize.Y ChatLog.ScrollingDirection=Enum.ScrollingDirection.Y ChatLog.Visible=true local CommandsLog=Instance.new("ScrollingFrame")CommandsLog.Name="CommandsLog"CommandsLog.Parent=LogContainer CommandsLog.BackgroundTransparency=1 CommandsLog.BorderSizePixel=0 CommandsLog.Size=UDim2.new(1,0,1,0)CommandsLog.ScrollBarThickness=4 CommandsLog.CanvasSize=UDim2.new(0,0,0,0)CommandsLog.AutomaticCanvasSize=Enum.AutomaticSize.Y CommandsLog.ScrollingDirection=Enum.ScrollingDirection.Y CommandsLog.Visible=false local NotificationFrame=Instance.new("Frame")NotificationFrame.Name="NotificationFrame"NotificationFrame.Parent=ChatGui NotificationFrame.BackgroundTransparency=1 NotificationFrame.Size=UDim2.new(1,0,1,0)local NotificationTemplate=Instance.new("Frame")NotificationTemplate.Name="NotificationTemplate"NotificationTemplate.BackgroundColor3=Color3.fromRGB(30,30,40)NotificationTemplate.BorderSizePixel=0 NotificationTemplate.Position=UDim2.new(0.5,-175,0.1,0)NotificationTemplate.Size=UDim2.new(0,350,0,70)NotificationTemplate.Visible=false NotificationTemplate.Parent=NotificationFrame local NotificationUICorner=Instance.new("UICorner")NotificationUICorner.CornerRadius=UDim.new(0,10)NotificationUICorner.Parent=NotificationTemplate local PlayerImage=Instance.new("ImageLabel")PlayerImage.Name="PlayerImage"PlayerImage.Parent=NotificationTemplate PlayerImage.BackgroundColor3=Color3.fromRGB(40,40,50)PlayerImage.Position=UDim2.new(0.05,0,0.2,0)PlayerImage.Size=UDim2.new(0,40,0,40)PlayerImage.BorderSizePixel=0 local PlayerImageUICorner=Instance.new("UICorner")PlayerImageUICorner.CornerRadius=UDim.new(0,20)PlayerImageUICorner.Parent=PlayerImage local PlayerName=Instance.new("TextLabel")PlayerName.Name="PlayerName"PlayerName.Parent=NotificationTemplate PlayerName.BackgroundTransparency=1 PlayerName.Position=UDim2.new(0.2,0,0.2,0)PlayerName.Size=UDim2.new(0.7,0,0.3,0)PlayerName.Font=Enum.Font.GothamSemibold PlayerName.TextColor3=Color3.fromRGB(255,255,255)PlayerName.TextSize=14 PlayerName.TextXAlignment=Enum.TextXAlignment.Left local ActionText=Instance.new("TextLabel")ActionText.Name="ActionText"ActionText.Parent=NotificationTemplate ActionText.BackgroundTransparency=1 ActionText.Position=UDim2.new(0.2,0,0.5,0)ActionText.Size=UDim2.new(0.75,0,0.4,0)ActionText.Font=Enum.Font.Gotham ActionText.TextColor3=Color3.fromRGB(200,200,255)ActionText.TextSize=12 ActionText.TextXAlignment=Enum.TextXAlignment.Left local settings={trackEnabled=true,notificationsEnabled=true,freezeOnTeleport=true,chatLogEnabled=true,commandLogEnabled=true}local function createNotification(player,action,color)if not settings.notificationsEnabled then return end local newNotification=NotificationTemplate:Clone()newNotification.Visible=true newNotification.Parent=NotificationFrame local userId=player.UserId local thumbType=Enum.ThumbnailType.HeadShot local thumbSize=Enum.ThumbnailSize.Size420x420 local content,isReady=Players:GetUserThumbnailAsync(userId,thumbType,thumbSize)newNotification.PlayerImage.Image=content newNotification.PlayerName.Text=player.Name newNotification.ActionText.Text=action newNotification.ActionText.TextColor3=color or Color3.fromRGB(255,255,255)newNotification.Position=UDim2.new(0.5,-175,0.1,-100)local tweenIn=TweenService:Create(newNotification,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Position=UDim2.new(0.5,-175,0.1,0)})tweenIn:Play()wait(3)local tweenOut=TweenService:Create(newNotification,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.In),{Position=UDim2.new(0.5,-175,0.1,-100)})tweenOut:Play()tweenOut.Completed:Connect(function()newNotification:Destroy()end)end local function freezePlayer(duration)local character=LocalPlayer.Character if not character then return end local humanoid=character:FindFirstChildOfClass("Humanoid")if not humanoid then return end if humanoid:FindFirstChild("FlyEnabled")then humanoid.FlyEnabled=false end if humanoid.Sit then humanoid.Sit=false wait(0.2)end local original={WalkSpeed=humanoid.WalkSpeed,JumpPower=humanoid.JumpPower,AutoRotate=humanoid.AutoRotate}humanoid.WalkSpeed=0 humanoid.JumpPower=0 humanoid.AutoRotate=false local startTime=os.clock()while os.clock()-startTime<duration do RunService.Heartbeat:Wait()if not character or not character:FindFirstChild("HumanoidRootPart")then break end character.HumanoidRootPart.Anchored=true end if character and character:FindFirstChild("HumanoidRootPart")then character.HumanoidRootPart.Anchored=false end if humanoid then humanoid.WalkSpeed=original.WalkSpeed humanoid.JumpPower=original.JumpPower humanoid.AutoRotate=original.AutoRotate end end local function teleportPlayer(position)local character=LocalPlayer.Character if character and character:FindFirstChild("HumanoidRootPart")then character.HumanoidRootPart.CFrame=CFrame.new(position)end end local function addMessage(panel,player,message,color)local isPrivate=string.sub(message,1,3)=="/w "local messageFrame=Instance.new("Frame")messageFrame.BackgroundTransparency=1 messageFrame.Size=UDim2.new(1,-10,0,20)messageFrame.Position=UDim2.new(0,5,0,#panel:GetChildren()*22)messageFrame.Parent=panel local timeLabel=Instance.new("TextLabel")timeLabel.Text="["..os.date("%H:%M:%S").."] "timeLabel.Size=UDim2.new(0,70,1,0)timeLabel.Font=Enum.Font.RobotoMono timeLabel.TextSize=12 timeLabel.TextColor3=Color3.fromRGB(150,150,150)timeLabel.BackgroundTransparency=1 timeLabel.TextXAlignment=Enum.TextXAlignment.Left timeLabel.Parent=messageFrame local playerLabel=Instance.new("TextLabel")playerLabel.Text=player.Name..(isPrivate and"(Private)"or"")..": "playerLabel.Size=UDim2.new(0,120,1,0)playerLabel.Position=UDim2.new(0,70,0,0)playerLabel.Font=Enum.Font.GothamSemibold playerLabel.TextSize=12 playerLabel.TextColor3=isPrivate and Color3.fromRGB(255,150,255)or Color3.fromRGB(100,200,255)playerLabel.BackgroundTransparency=1 playerLabel.TextXAlignment=Enum.TextXAlignment.Left playerLabel.Parent=messageFrame local messageLabel=Instance.new("TextLabel")messageLabel.Text=message messageLabel.Size=UDim2.new(1,-190,1,0)messageLabel.Position=UDim2.new(0,190,0,0)messageLabel.Font=Enum.Font.Gotham messageLabel.TextSize=12 messageLabel.TextColor3=color or(isPrivate and Color3.fromRGB(255,200,255)or Color3.fromRGB(220,220,220))messageLabel.BackgroundTransparency=1 messageLabel.TextXAlignment=Enum.TextXAlignment.Left messageLabel.TextWrapped=true messageLabel.Parent=messageFrame end local function switchTab(tabName)ChatTab.BackgroundColor3=tabName=="chat"and Color3.fromRGB(50,50,60)or Color3.fromRGB(35,35,40)CommandsTab.BackgroundColor3=tabName=="commands"and Color3.fromRGB(50,50,60)or Color3.fromRGB(35,35,40)ChatLog.Visible=tabName=="chat"CommandsLog.Visible=tabName=="commands"end local function processChatMessage(player,message)local trackCommands={":track t",":track tü",":track tür",":track türk",":track tsk",":track #t",":track #tü",":track #tür",":track #türk",":track #tsk"}local untrackCommand=":untrack"local function cleanCommand(cmd)return string.lower(string.gsub(cmd,"%s+$",""))end local cleanedMsg=cleanCommand(message)local isCommand=string.sub(cleanedMsg,1,1)==":"local isTrackCommand=false local isUntrackCommand=(cleanedMsg==cleanCommand(untrackCommand))for _,cmd in ipairs(trackCommands)do if cleanedMsg==cleanCommand(cmd)then isTrackCommand=true break end end if settings.chatLogEnabled and not isCommand then addMessage(ChatLog,player,message)end if isCommand and settings.commandLogEnabled then local color=isTrackCommand and Color3.fromRGB(150,255,150)or isUntrackCommand and Color3.fromRGB(255,100,100)or Color3.fromRGB(255,150,150)addMessage(CommandsLog,player,message,color)end if isTrackCommand and settings.trackEnabled then teleportPlayer(Vector3.new(1700,13,-462))freezePlayer(2)teleportPlayer(Vector3.new(3120,35,-136))createNotification(player,"Sizi takip etti ve ışınlandınız!",Color3.fromRGB(100,255,100))elseif isUntrackCommand and settings.trackEnabled then createNotification(player,"Takibi bıraktı!",Color3.fromRGB(255,100,100))end end CloseButton.MouseButton1Click:Connect(function()ChatGui:Destroy()end)MinimizeButton.MouseButton1Click:Connect(function()ContentFrame.Visible=not ContentFrame.Visible MainWindow.Size=ContentFrame.Visible and UDim2.new(0,350,0,250)or UDim2.new(0,350,0,30)end)ChatTab.MouseButton1Click:Connect(function()switchTab("chat")end)CommandsTab.MouseButton1Click:Connect(function()switchTab("commands")end)TrackToggle.MouseButton1Click:Connect(function()settings.trackEnabled=not settings.trackEnabled TrackToggle.Text="TRACK:"..(settings.trackEnabled and"ON"or"OFF")TrackToggle.TextColor3=settings.trackEnabled and Color3.fromRGB(150,255,150)or Color3.fromRGB(255,150,150)end)for _,player in ipairs(Players:GetPlayers())do player.Chatted:Connect(function(msg)processChatMessage(player,msg)end)end Players.PlayerAdded:Connect(function(player)player.Chatted:Connect(function(msg)processChatMessage(player,msg)end)end)switchTab("chat")MinimizeButton.MouseButton1Click:Wait()
